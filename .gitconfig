[includeIf "gitdir:~/Documents/GitHub/"]
	path = .gitconfig_private
[includeIf "gitdir:~/Documents/Bitbucket/"]
	path = .gitconfig_work

[alias]
	a = add
	b = branch
	bd = branch -D
	c = commit
	ca = commit --amend
	cl = clone
	cm = commit -m
	co = checkout
	d = diff
	l = log
	p = push
	pl = pull
	rh = reset --hard
	s = status
	sh = stash
